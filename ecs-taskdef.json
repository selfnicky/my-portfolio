{
  "family": "utc-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::487042309881:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "appnginx-fargate-task",
      "image": "487042309881.dkr.ecr.us-east-1.amazonaws.com/dev@sha256:259042b7600089e36a29c8dacc30033bfaadbcb5c8f3b2bd8568adcbd00915f9",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/utc-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
